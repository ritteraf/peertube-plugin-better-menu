{
  "name": "peertube-plugin-menu-items",
  "description": "PeerTube plugin menu-items",
  "version": "0.0.6",
  "author": "kontrollanten",
  "bugs": "https://framagit.org/kontrollanten/peertube-plugin-menu-items/issues",
  "clientScripts": [
    {
      "script": "dist/client/common-client-plugin.js",
      "scopes": [
        "common"
      ]
    }
  ],
  "css": [],
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "esbuild": "^0.21.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  },
  "engine": {
    "peertube": ">=1.3.0"
  },
  "homepage": "https://framagit.org/kontrollanten/peertube-plugin-menu-items",
  "keywords": [
    "peertube",
    "plugin"
  ],
  "library": "./dist/server/server/main.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "prepare": "npm run build",
    "build:client": "node ./scripts/build.cjs",
    "build:server": "tsc --build server/tsconfig.json",
    "build": "npm-run-all -s clean check:client:tsc -p build:server build:client",
    "check:client:tsc": "npx tsc --p client/tsconfig.json --noEmit",
    "lint:script": "npx eslint --ext .ts --ext .js ."
  },
  "staticDirs": {},
  "translations": {},
  "dependencies": {
    "@peertube/peertube-types": "^6.1.0"
  }
}
